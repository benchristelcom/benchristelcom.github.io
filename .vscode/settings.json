{
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "src/log.md",
        "cmd": "marss src/log.md src/feed.rss && cp src/feed.rss dist/feed.rss"
      },
      {
        "match": ".*",
        "cmd": "./build"
      },
    ]
  },
  "files.exclude": {
    "docs": true,
    "public": true
  }
}