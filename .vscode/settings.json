{
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "log.md",
        "cmd": "marss log.md src/feed.rss src/feed.xml src/log.html"
      },
      {
        "match": ".*",
        "cmd": "./build"
      },
    ]
  },
  "files.exclude": {
    "docs": true,
    "public": true
  }
}